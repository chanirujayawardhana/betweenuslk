{
  "rewrites": [
    { "source": "/(.*)", "destination": "/maintenance.html" }
  ],
  "headers": [
    {
      "source": "/maintenance.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" },
        { "key": "X-Robots-Tag", "value": "noindex, nofollow" }
      ]
    }
  ]
}
